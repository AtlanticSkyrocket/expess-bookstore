{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "isbn",
      "amazon_url",
      "author",
      "language",
      "pages",
      "publisher",
      "title",
      "year"
  ],
  "properties": {
      "isbn": {
          "type": "string",
          "default": "",
          "title": "The isbn Schema",
          "examples": [
              "9780451524935"
          ]
      },
      "amazon_url": {
          "type": "string",
          "default": "",
          "title": "The amazon_url Schema",
          "examples": [
              "https://www.amazon.com/dp/0451524934"
          ]
      },
      "author": {
          "type": "string",
          "default": "",
          "title": "The author Schema",
          "examples": [
              "George Orwell"
          ]
      },
      "language": {
          "type": "string",
          "default": "",
          "title": "The language Schema",
          "examples": [
              "English"
          ]
      },
      "pages": {
          "type": "integer",
          "default": 0,
          "title": "The pages Schema",
          "examples": [
              328
          ]
      },
      "publisher": {
          "type": "string",
          "default": "",
          "title": "The publisher Schema",
          "examples": [
              "Signet Classics"
          ]
      },
      "title": {
          "type": "string",
          "default": "",
          "title": "The title Schema",
          "examples": [
              "1984"
          ]
      },
      "year": {
          "type": "integer",
          "default": 0,
          "title": "The year Schema",
          "examples": [
              1949
          ]
      }
  },
  "examples": [{
      "isbn": "9780451524935",
      "amazon_url": "https://www.amazon.com/dp/0451524934",
      "author": "George Orwell",
      "language": "English",
      "pages": 328,
      "publisher": "Signet Classics",
      "title": "1984",
      "year": 1949
  }]
}